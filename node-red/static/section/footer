function createThread(path) {
    let library = {
        avatar: Object.assign( {}, _avatar),
        buy_response: Object.assign( {}, _buy_response),
        buy_text : Object.assign( {}, _buy_text),
        review_image : Object.assign( {}, _review_image[path]),
        review_response : Object.assign( {}, _review_response),
        review_text : Object.assign( {}, _review_text),
        profile_link : Object.assign( {}, _profile_link)

    }
    function Random(obj) {
        let _obj = Object.fromEntries(Object.entries(library[obj]));
        let index = Math.round( Math.random()*(Object.keys(_obj).length-1) );
        let value = _obj[index];
        delete _obj[index];
        library[obj] = Object.values(_obj);
        console.log(_obj);
        return value; 
    }
    let time = 28;
    let uptime = 18
    function createComment() {
        let _avatar = Random("avatar");
        time += Math.round(Math.random() * uptime + 1);
        uptime += Math.random() * uptime
        let comment = {
            thumbnail: _avatar[1],
            name: _avatar[0],
            link: Random("profile_link"),
            time: (time < 60) ? `${time} phút` : `${Math.floor( time /60)} giờ`,
        };
        time += Math.round(Math.random() * 8 + 1);
        if ( Math.random() > 0.5 ){
            comment["text"] = Random("review_text");
            let new_review_image = Random("review_image");
            if ( new_review_image) {
                comment["media"] = new_review_image;
            };
            comment["replies"] = [{
                text: Random("review_response"),
                time: (time < 60) ? `${time} phút` : `${Math.floor( time /60)} giờ`,
            }]
        } else {
            comment["text"] = Random("buy_text");
            comment["replies"] = [{
                text: Random("buy_response"),
                time: (time < 60) ? `${time} phút` : `${Math.floor( time /60)} giờ`,
            }]
        };
        return comment;
    };

    return ({
        isDisplay: true,
        icon_url: "https://w.ladicdn.com/s250x250/5b39a35979c7e420514fbfc4/icon-fb-20200627185646.png",
        reaction_amount: `${Random("avatar")[0]} và ${ 12342 + Math.round( Math.random() * 5000)} người khác`,
        comment_amount: `${ 8234 + Math.round( Math.random() * 3000)} Bình Luận`,
        comment_wait: "Ai đó đang nhập bình luận...",
        fanpge_name: "Cơ Khí Nhập Khẩu",
        fanpage_link: "https://www.facebook.com/profile.php?id=100079822181597",
        fanpage_thumbnail: "https://statics.pancake.vn/web-media/0a/ad/4c/09/d7a30b72e6de59e301b37f3079558d8803d4ad0a286970a359bdb05e.jpg",

        comments:  [... Array(Math.round( Math.random() * 2) + 5).keys()].map( () => {
            let comment = createComment();
            if (Math.random() > 0.5 ) {
                let addCommentReply = createComment();
                let replyAddCommentreply = addCommentReply.replies[0];
                delete addCommentReply.replies;
                comment.replies.push(addCommentReply)
                comment.replies.push(replyAddCommentreply)
            }
            return comment;
    })})
};
if (_review_image.hasOwnProperty(__req.path)) {
    data_comment = createThread(__req.path);
}
